
public class arrayPractice {
	public static void main(String[] args){
		//array A
		int[] arrayA = new int [10];
		for(int i = 0; i < 10; i++){
			arrayA[i] = (i+1);
		}
		System.out.print("Array A:");
		for(int i = 0; i < 10; i++){
			System.out.print(" " + arrayA[i]);
		}
		System.out.println();
	}
}
